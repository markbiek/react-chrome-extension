{
  "manifest_version": 2,

  "name": "Simple Extension",
  "description": "This is a simple extension that uses ReactJS for the UI",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "identity",
    "identity.email",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
